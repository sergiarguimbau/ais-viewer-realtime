{
  "name": "ais-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "project": "docker compose up --build",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "lint": "biome check .",
    "lint:fix": "biome check --write ."
  },
  "dependencies": {
    "dotenv": "16.5.0",
    "drizzle-orm": "0.44.1",
    "fastify": "5.3.3",
    "pg": "8.16.0",
    "ws": "8.18.2",
    "zod": "3.25.49"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "22.15.29",
    "@types/pg": "8.15.4",
    "@types/ws": "8.18.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.8.3"
  }
}
